{
  "name": "vue-gate",
  "version": "0.1.0",
  "description": "Vue Authorize Plugin",
  "main": "dist/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --open",
    "dev": "cross-env NODE_ENV=development webpack --colors --progress --hide-modules",
    "prod": "cross-env NODE_ENV=production webpack --colors --progress --hide-modules",
    "test": "jest",
    "lint": "eslint ./*.js src/",
    "fix": "eslint ./*.js src/ --fix"
  },
  "keywords": [
    "vue",
    "authorize"
  ],
  "author": "adenvt@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "^8.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.5.0",
    "eslint-config-adenvt": "^1.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-unicorn": "^6.0.0",
    "eslint-plugin-varspacing": "^1.2.1",
    "eslint-plugin-vue": "^4.7.1",
    "jest": "^23.5.0",
    "pre-commit": "^1.2.2",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7"
  },
  "pre-commit": [
    "lint"
  ]
}
